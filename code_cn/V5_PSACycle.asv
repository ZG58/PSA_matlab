        A0 = zeros(N+2) ;
	%   
    %% 基于独立分块创建整体Jacobian
        J_ads = [ A4, A4, A1, A1, A4;... 
                  A4, A4, A1, A1, A4;... 
                  A1, A1, A1, A0, A1;... 
                  A1, A1, A0, A1, A1;... 
                  A4, A1, A1, A1, A4 ]  ;
	%   
    %% 基于边界条件进行修改
        % 压力入口
        J_ads(1, :) = 0 ;
        J_ads(:, 1) = 0 ;
        
        % 压力出口
        J_ads(N+2, :) = 0 ;
        J_ads(:, N+2) = 0 ;
        
        % 摩尔分数入口
        J_ads(N+3, :) = 0 ;
        J_ads(:, N+3) = 0 ;
        
